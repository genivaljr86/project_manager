<%= form_for(@project) do |f| %>
  <%= error_messages_for @project %>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title, size: 60 %>
  </div>
  <div class="field">
    <%= f.label :customer_name, "Cliente" %><br />
    <%= f.text_field :customer_name, data: {autocomplete_source: customers_path}  %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
